li class=\"nav-bar-list-item\" @click=\"$router.push('/clients')\">\n            Мои клиенты\n          </li>\n\n          <li class=\"nav-bar-list-item\" @click=\"$router.push('/calendar')\">\n            Календарь\n          </li>\n\n          <li class=\"nav-bar-list-item\" @click=\"$router.push('/results')\">\n            Отчёты по встречам\n          </li>\n\n          <li class=\"nav-bar-list-item button-exit\" @click=\"logOutWrapper\">\n            <button class=\"log-out-item\">Выйти</button>\n          </li>\n        </div>\n      </ul>\n\n      <button class=\"nav-bar-notify-button\">\n        <img src=\"/svg/notify.svg\" alt=\"\" class=\"nav-bar-notify-icon\" />\n      </button>\n\n      <button class=\"nav-bar-add-meet\">\n        <img src=\"/svg/add-meet.svg\" alt=\"\" class=\"nav-bar-icon-add\" />\n      </button>\n\n      <button class=\"nav-bar-profile\">\n        <img src=\"/svg/profile.svg\" alt=\"\" class=\"nav-bar-profile-icon\" />\n      </button>\n\n      <button @click=\"logOutWrapper\" class=\"log-out\">Выйти</button>\n    </nav>\n\n    <hr class=\"line-header\" />\n  </header>\n</template>\n\n<script>\nimport { mapMutations } from \"vuex\";\n\nexport default {\n  components: {},\n  data: () => ({\n    width: 0,\n  }),\n  methods: {\n    ...mapMutations([\"logOut\"]),\n\n    logOutWrapper() {\n      this.$router.push(\"/login\");\n      this.logOut();\n    },\n  },\n};\n</script>\n\n<style scoped>\n\n@import url('../assests/fonts/Exo_2/stylesheet.css');\n\nbutton {\n  background-color: white;\n  border: none;\n  margin: 0;\n  padding: 0;\n}\n\n.header {\n  background-color: #fff;\n}\n\n* {\n  padding: 0;\n  margin: 0;\n  font-family: 'Exo 2';\n  box-sizing: border-box;\n  font-weight: 700;\n}\n\n.log-out {\n  font-size: 1rem;\n}\n\n.logo-name {\n  font-weight: 900;\n  font-size: 1.5rem;\n}\n\n.menu-items {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n}\n\nhtml,\nbody {\n  overflow-x: hidden;\n}\n.button-exit {\n  display: none;\n}\nnav {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  min-height: 110px;\n  margin-left: 200px;\n  margin-right: 200px;\n}\n\n.nav-bar-list {\n  display: flex;\n  justify-content: space-around;\n  width: 50%;\n}\n\n.nav-bar-list li {\n  list-style: none;\n}\n\n.nav-bar-list {\n  display: flex;\n  justify-content: space-around;\n}\n\n.nav-bar-icon-button {\n  display: flex;\n  margin-left: auto;\n  justify-content: center;\n  align-items: center;\n}\n\n.nav-bar-icon-button-two {\n  margin-left: auto;\n}\n\n.title-name {\n  margin-right: 60px;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n}\n\n.line-header {\n  width: 100%;\n}\n\n.nav-bar-list-item {\n  font-size: 16px;\n  margin-right: 52px;\n}\n\n.nav-bar-add-meet {\n  margin-right: 34px;\n}\n\n.log-out {\n  background-color: white;\n  border: none;\n  margin: 0;\n  padding: 0;\n}\n\n.log-out-item {\n  background-color: #90dc97;\n  border: none;\n  font-size: 16px;\n  margin: 0;\n  padding: 0;\n}\n\n.nav-bar-notify-button {\n  height: 40px;\n\n  background-color: white;\n  border: none;\n  margin: 0;\n  padding: 0;\n}\n\n.nav-bar-add-meet {\n  height: 60px;\n\n  background-color: white;\n  border: none;\n  margin-top: 0;\n  margin-bottom: 0;\n  margin-left: 0;\n  padding: 0;\n  display: flex;\n}\n\n.nav-bar-profile {\n  height: 70px;\n\n  background-color: white;\n  border: none;\n  margin-top: 0;\n  margin-bottom: 0;\n  margin-left: 0;\n  padding: 0;\n  margin-right: 20px;\n}\n\n.nav-bar-icon-button {\n  display: flex;\n  justify-content: space-around;\n  width: 20%;\n}\n\n#checkbox_toggle {\n  display: none;\n}\n\n.hamburger {\n  display: none;\n  font-size: 50px;\n  cursor: pointer;\n}\n\n.hamburger-line {\n  background-color: #47af52;\n  display: block;\n  height: 2px;\n  position: relative;\n  width: 24px;\n}\n\n.hamburger-line::before,\n.hamburger-line::after {\n  background-color: #47af52;\n  content: \"\";\n  display: block;\n  position: absolute;\n  transition: all 0.3s;\n  width: 100%;\n  height: 100%;\n}\n\n.hamburger-line::before {\n  top: 8px;\n}\n\n.hamburger-line::after {\n  top: -8px;\n}\n\n@media (max-width: 1200px) {\n  nav {\n    margin: 0;\n    display: flex;\n    flex-direction: row-reverse;\n  }\n\n  nav .nav-bar-notify-button {\n    margin-right: auto;\n  }\n  .button-exit {\n    display: block;\n  }\n\n  .log-out-item {\n    font-size: 24px;\n  }\n\n  .menu-items li {\n    transition: all 0.5s;\n    display: block;\n    padding: 25px;\n    font-size: 24px;\n    margin-right: 0;\n    border: 1px solid white;\n  }\n  .menu-items li:hover {\n    background-color: #46d554;\n  }\n\n  .title-name,\n  .log-out {\n    display: none;\n  }\n\n  .menu-items {\n    display: none;\n    max-height: 0;\n    overflow: hidden;\n    position: absolute;\n    background-color: #90dc97;\n    text-align: center;\n    right: 0;\n    left: 0;\n    margin-top: 77px;\n  }\n\n  .hamburger {\n    display: block;\n    padding: 20px 0;\n    margin-right: 10px;\n    margin-left: auto;\n  }\n  #checkbox_toggle:checked ~ .menu-items {\n    display: block;\n    max-height: 100%;\n    transition: all 0.5s;\n  }\n\n  #checkbox_toggle:checked ~ .hamburger .hamburger-line {\n    background-color: transparent;\n  }\n\n  #checkbox_toggle:checked ~ .hamburger .hamburger-line::before {\n    transform: rotate(-45deg);\n    top: 0;\n  }\n  #checkbox_toggle:checked ~ .hamburger .hamburger-line::after {\n    transform: rotate(45deg);\n    top: 0;\n  }\n}\n\n/* Hovers and animations */\n\n.logo-name:hover,\n.log-out:hover {\n  opacity: 0.9;\n  transition: 0.5s;\n  color: #00B268;\n}\n\n.nav-bar-list-item:hover {\n  opacity: 0.9;\n  transition: 0.5s;\n  color: #00B268;\n}\n\n.nav-bar-icon-add:hover,\n.nav-bar-profile-icon:hover {\n  opacity: 0.7;\n  transition: 0.5s;\n}\n\n.nav-bar-notify-button:hover {\n  transition: background 0.001s ease 0.3s;\n  background-image: #00B268;\n}\n</style>"]},"metadata":{},"sourceType":"module"}