{"ast":null,"code":"import buttons from \"@/components/UI/button-add.vue\";\nimport api from \"@/api\";\nexport default {\n  components: {\n    buttons\n  },\n\n  data() {\n    return {\n      meetings: [],\n      nearMeeting: {}\n    };\n  },\n\n  async mounted() {\n    this.meetings = await api.getMeetings();\n    this.prepareClientInMeeting();\n    this.nearMeeting = this.meetings[0];\n  },\n\n  methods: {\n    async prepareClientInMeeting() {\n      const clients = await api.getClients();\n      const mathchIdToClient = new Map();\n      clients.forEach(item => {\n        mathchIdToClient.set(item.id, item);\n      });\n      this.meetings.forEach(meeting => {\n        meeting.client = mathchIdToClient.get(meeting.client).company;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmDA,OAAOA,OAAP,MAAoB,gCAApB;AACA,OAAOC,GAAP,MAAgB,OAAhB;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEF;EAAF,CADC;;EAGbG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,WAAW,EAAE;IAFR,CAAP;EAID,CARY;;EAUb,MAAMC,OAAN,GAAgB;IACd,KAAKF,QAAL,GAAgB,MAAMH,GAAG,CAACM,WAAJ,EAAtB;IACA,KAAKC,sBAAL;IACA,KAAKH,WAAL,GAAmB,KAAKD,QAAL,CAAc,CAAd,CAAnB;EACD,CAdY;;EAgBbK,OAAO,EAAE;IACP,MAAMD,sBAAN,GAA+B;MAC7B,MAAME,OAAM,GAAI,MAAMT,GAAG,CAACU,UAAJ,EAAtB;MACA,MAAMC,gBAAe,GAAI,IAAIC,GAAJ,EAAzB;MACAH,OAAO,CAACI,OAAR,CAAiBC,IAAD,IAAU;QACxBH,gBAAgB,CAACI,GAAjB,CAAqBD,IAAI,CAACE,EAA1B,EAA8BF,IAA9B;MACD,CAFD;MAIA,KAAKX,QAAL,CAAcU,OAAd,CAAsBI,OAAM,IAAK;QAC/BA,OAAO,CAACC,MAAR,GAAiBP,gBAAgB,CAACQ,GAAjB,CAAqBF,OAAO,CAACC,MAA7B,EAAqCE,OAAtD;MACD,CAFD;IAGD;;EAXM;AAhBI,CAAf","names":["buttons","api","components","data","meetings","nearMeeting","mounted","getMeetings","prepareClientInMeeting","methods","clients","getClients","mathchIdToClient","Map","forEach","item","set","id","meeting","client","get","company"],"sourceRoot":"","sources":["/Users/root1/Developments/SberMeetingsWeb/WebApplication/vue-project/src/components/main.vue"],"sourcesContent":["<template>\n  <main>\n    <div class=\"container\">\n      <div class=\"near-meet\">\n        <h4 class=\"near-meet-text\">Ближайшая встреча:</h4>\n\n        <div class=\"window-near-meet\">\n          <div class=\"container-window\">\n            <button class=\"pencil-meet\">\n              <img src=\"/svg/pencil.svg\" alt=\"\" class=\"pencil-icon\" />\n            </button>\n            <p class=\"client-wiindow\">{{nearMeeting.client}}</p>\n            <div class=\"window-tel-and-contact\">\n              <p class=\"contact-window\">{{nearMeeting.contact}}</p>\n              <p class=\"tel-window\">Телефон</p>\n            </div>\n            <p class=\"theme-window\">{{nearMeeting.topic}}</p>\n            <div class=\"data-and-time-window\">\n              <p class=\"data-window\">Дата</p>\n              <p class=\"time-window\">Время</p>\n            </div>\n            <p class=\"place-window\">Место</p>\n          </div>\n        </div>\n        <div class=\"green-button-add\">\n          <buttons></buttons>\n        </div>\n      </div>\n\n      <div class=\"list-meet\">\n        <h4 class=\"list-meet-text\">Список встреч</h4>\n\n        <div class=\"all-meet\">\n          <ul class=\"list-all-meet\">\n            <li v-for=\"meeting in meetings\" :key=\"meeting\" class=\"all-meet-item\">\n              <div class=\"data-time-place-item\">\n                <p class=\"data-item\">{{ meeting.date }}</p>\n                <p class=\"time-item\">{{ meeting.start }} - {{ meeting.end }}</p>\n                <p class=\"place-item\">{{ meeting.place }}</p>\n              </div>\n              <p class=\"theme-item\">{{ meeting.topic }}</p>\n              <p class=\"client-item\">{{ meeting.client }}</p>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  </main>\n</template>\n\n<script>\nimport buttons from \"@/components/UI/button-add.vue\";\nimport api from \"@/api\";\n\nexport default {\n  components: { buttons },\n\n  data() {\n    return {\n      meetings: [],\n      nearMeeting: {}\n    };\n  },\n\n  async mounted() {\n    this.meetings = await api.getMeetings();\n    this.prepareClientInMeeting();\n    this.nearMeeting = this.meetings[0];\n  },\n\n  methods: {\n    async prepareClientInMeeting() {\n      const clients = await api.getClients();\n      const mathchIdToClient = new Map();\n      clients.forEach((item) => {\n        mathchIdToClient.set(item.id, item);\n      });\n\n      this.meetings.forEach(meeting => {\n        meeting.client = mathchIdToClient.get(meeting.client).company;\n      });\n    },\n  },\n};\n</script>\n\n<style>\n.window-near-meet {\n  width: 774px;\n  height: 567px;\n  background: #ffffff;\n  border: 1px solid #bdbdbd;\n  border-radius: 30px;\n  margin-bottom: 60px;\n}\n\np {\n  font-size: 20px;\n  margin: 0;\n}\n.near-meet-text {\n  margin-top: 31px;\n  margin-bottom: 31px;\n}\n\n.near-meet {\n  margin-right: 40px;\n}\n.green-button-add {\n  display: flex;\n  justify-content: center;\n}\n\n.data-time-place-item {\n  display: flex;\n}\n\n.window-tel-and-contact {\n  display: flex;\n}\n\n.pencil-meet {\n  margin-top: 38px;\n\n  margin-right: 23px;\n  margin-left: auto;\n  display: flex;\n}\n\n.all-meet-item {\n  width: 356px;\n  height: 127px;\n  background: #f1fbf2;\n  border: 0.714062px solid #f1fbf2;\n  box-shadow: 0px 2.85625px 2.85625px rgba(0, 0, 0, 0.25);\n  border-radius: 10px;\n}\n\n.all-meet-item {\n  margin-bottom: 19px;\n}\n\n.contact-window {\n  margin-right: 334px;\n  margin-bottom: 53px;\n}\n\n.data-and-time-window {\n  display: flex;\n}\n\n.container-window {\n  margin-left: 40px;\n}\n\n.container {\n  display: flex;\n}\n\n.all-meet {\n  width: 406px;\n  height: 766px;\n  background: #00b268;\n  border-radius: 20px;\n  overflow: auto;\n}\n.theme-window {\n  margin-bottom: 123px;\n}\n\n.list-all-meet {\n  margin-left: 16px;\n}\n\n.data-window {\n  margin-bottom: 18px;\n  margin-right: 439px;\n}\n.client-wiindow {\n  margin-bottom: 26px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}